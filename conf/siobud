<VirtualHost *:80>
	DocumentRoot /home/sean/public_html/siobud.com/www
	ProxyPass /css !
	ProxyPass /resume !
	ProxyPass /img !
	ProxyPass /js !
	ProxyPass /reload !
	ProxyPass /files !
	
	ProxyPass / http://localhost:8080/ retry=0
	ProxyPassReverse / http://localhost:8080/ retry=0

	Options -Indexes
	CustomLog /home/sean/public_html/logs/siobud.com_access.log common
	<IfModule mpm_itk_module>
  		AssignUserId sean users
  	</IfModule>
</VirtualHost>
